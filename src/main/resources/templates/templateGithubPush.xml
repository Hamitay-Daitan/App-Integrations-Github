<messageML>
    <div class="entity">
        <card class="barStyle">
            <header>
                <span class="tempo-text-color--normal">${entity['githubPush'].refType} </span>
                <a href="${entity['githubPush'].compare}">${entity['githubPush'].ref} </a>
                <span class="tempo-text-color--normal">at </span>
                <a href="${entity['githubPush'].repository.url}">${entity['githubPush'].repository.fullName} </a>
                <span class="tempo-text-color--green"><strong>pushed </strong></span>
                <span class="tempo-text-color--normal">by </span>
                <span class="tempo-text-color--normal"><strong>${entity['githubPush'].pusher.name} </strong></span>
            </header>
        </card>
    </div>
</messageML>
